  "description": "a tiny script transpiles json exported by Katalon web recorder to puppeteer script.",
  "scripts": {
    "build": "tsc --build tsconfig.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
      "k2p": "bin/index.js"
  },
  "files": [
    "bin/**/*.js",
    "dist/**/*.js"
  ],
  "keywords": [
    "katalon",
    "node"
  ],
  "author": "Yusei Abe",
  "repository": {
